package classHerenciaConstructor;

public class alumnosMain {
    public static void main(String[] args){
        System.out.println("CLASE HIJA HOMBRES");
        hombres hombres = new hombres(30, 50, 80, 37);
        System.out.println(hombres.getPorcentajeHombres());

        System.out.println("CLASE HIJA MUJERES");
        mujeres mujeres = new mujeres(30, 50, 80, 62);
        System.out.println(mujeres.getPorcentajeMujeres());
    }

}
